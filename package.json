{
  "name": "martinlabs-blog",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env SOCKET=true node ./scripts/next-remote-watch.js ./data",
    "dev": "next dev",
    "build": "next build && node ./scripts/generate-sitemap",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint --fix --dir pages --dir components --dir lib --dir layouts --dir scripts",
    "lint:format": "prettier --check '**/*'",
    "lint:fix": "yarn -s lint --fix && yarn -s lint:format --write",
    "prepare": "husky install",
    "type:checks": "tsc",
    "dedupe:dependencies": "yarn-deduplicate -s fewer yarn.lock && yarn install"
  },
  "dependencies": {
    "@fontsource/poppins": "5.0.2",
    "@fontsource/prompt": "5.0.2",
    "@mailchimp/mailchimp_marketing": "3.0.80",
    "@radix-ui/react-collapsible": "^1.0.3",
    "@radix-ui/react-icons": "^1.3.0",
    "@tailwindcss/forms": "0.5.3",
    "@tailwindcss/typography": "0.5.9",
    "autoprefixer": "10.4.14",
    "esbuild": "0.17.17",
    "github-slugger": "1.5.0",
    "gray-matter": "4.0.3",
    "image-size": "1.0.2",
    "mdx-bundler": "9.2.1",
    "next": "13.4.4",
    "next-themes": "0.2.1",
    "postcss": "8.4.24",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "reading-time": "1.5.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-citation": "1.0.1",
    "rehype-preset-minify": "6.0.0",
    "rehype-prism-plus": "1.5.1",
    "rehype-slug": "5.1.0",
    "remark-footnotes": "4.0.1",
    "remark-gfm": "3.0.1",
    "remark-math": "5.1.1",
    "tailwindcss": "3.3.2",
    "unist-util-visit": "4.1.2"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "13.4.4",
    "@svgr/webpack": "8.0.1",
    "@types/react": "18.3.3",
    "@types/tailwindcss": "3.1.0",
    "@typescript-eslint/eslint-plugin": "5.59.9",
    "@typescript-eslint/parser": "5.59.9",
    "cross-env": "7.0.3",
    "dedent": "0.7.0",
    "eslint": "8.42.0",
    "eslint-config-next": "13.4.4",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-prettier": "4.2.1",
    "file-loader": "6.2.0",
    "globby": "11.1.0",
    "husky": "8.0.3",
    "inquirer": "9.2.7",
    "lint-staged": "13.2.2",
    "next-remote-watch": "2.0.0",
    "prettier": "2.8.8",
    "prettier-plugin-tailwindcss": "0.3.0",
    "socket.io": "4.6.2",
    "socket.io-client": "4.6.2",
    "stylelint": "^15.7.0",
    "typescript": "5.1.3",
    "yarn-deduplicate": "^6.0.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
